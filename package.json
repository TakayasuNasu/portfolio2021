{
  "description": "Typscript-Gatsby",
  "author": "Takayasu Nasu <taka.beckham@gmail.com> (https://www.i-nasu.com)",
  "private": true,
  "scripts": {
    "start": "NODE_OPTIONS=--max-old-space-size=4096 gatsby develop -H 0.0.0.0",
    "develop": "gatsby develop -H 0.0.0.0",
    "clean": "gatsby clean",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "postbuild": "rm -rf /workspace/public/ && cp -r public /workspace/public",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}' gatsby-*.ts && tsc --noEmit",
    "refresh": "rm -rf .cache node_modules public package-lock.json && npm install && npm run develop",
    "rebuild": "rm -rf .cache public && npm run develop"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/styled-components": "^5.1.28",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-cli": "^5.9.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "gatsby": "^5.9.0",
    "gatsby-plugin-anchor-links": "^1.2.1",
    "gatsby-plugin-image": "^3.9.0",
    "gatsby-plugin-manifest": "^5.9.0",
    "gatsby-plugin-mdx": "^5.9.0",
    "gatsby-plugin-root-import": "^2.0.9",
    "gatsby-plugin-sass": "^6.9.0",
    "gatsby-plugin-sharp": "^5.9.0",
    "gatsby-plugin-styled-components": "^6.9.0",
    "gatsby-source-contentful": "^8.9.1",
    "gatsby-source-filesystem": "^5.9.0",
    "gatsby-transformer-sharp": "^5.9.0",
    "rc-slider": "^10.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-intersection-observer": "^9.5.2",
    "sass": "^1.69.0",
    "styled-components": "^6.0.8",
    "styled-media-query": "^2.1.2",
    "swiper": "^10.3.1"
  }
}
